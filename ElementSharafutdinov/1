class ElementSharafutdinov:
    def __init__(self, name, symbol, number):
        self.name = name
        self.symbol = symbol
        self.number = number
    def __str__(self):
        return f'Element: {self.name}, Symbol: {self.symbol}, Number: {self.number}'
hydrogen = ElementSharafutdinov("Calcium", "Ca", 20)
print(hydrogen)
